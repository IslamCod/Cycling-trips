<div id="map" class="map mt-3" style="width: 155vh; height: 30vw"></div> <!-- подключение карты - Миля-->
<div class="card mb-3 container" style="max-width: 540px;">
  <div class="row g-0">
    <div class="col-md-4">
    <div id="list" class="col-md-8">
      {{#each allTrips}}
        <div class="card-body">
          <h5 class="card-title">Маршрут: {{this.title}}</h5>
<!-- убрала отсюда карту - Миля-->
          <p class="card-text">Автор маршрута: {{this.userName}}</p>
          <p class="card-text">Длина маршрута: {{this.length}}</p>
          <a href="#" name="roteInfo" id="{{id}}">{{routeName}}">Подробнее о
            маршруте</a>
          <p class="rating">{{trip.rating}}</p>
        </div>
      {{/each}}
    </div>
  </div>
</div>

<script src="/js/alltrips.js"></script> <!-- подключение скрипта для карты - Миля-->
